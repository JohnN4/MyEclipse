package com.jn.edu;

import java.util.Scanner;

/**
 * @author JohnN
 *
 */
public class IntroArrays {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
//		
//		int a[]= {12,31,82,24,1,17,3,99,81,16};
//		for(int i=0;i<10;i++)
//			System.out.println(a[i]);

		int a[]= {12,31,82,24,1,17,3,99,81,16};
		int max = a[0];
		for(int i = 1; i < a.length;i++)
		{
			if(a[i] > max)
			{
				max = a[i];
			}
			
			
		}
		
		System.out.println(max);
}
		
		
	
}