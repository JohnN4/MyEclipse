 * 
 */
package com.jn.edu;

/**
 * @author JohnN
 *
 */
public class Comparison {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a;
		int b;
		a=10;
		b=5;
		System.out.println("a="+a +"and b="+b);
		System.out.println("a == b:" +(a==b));
		System.out.println("a != b:" +(a!=b));
		System.out.println("a > b:" +(a>b));
		System.out.println("a >= b:" +(a>=b));
		System.out.println("a < b:" +(a<b));
		System.out.println("a <= b:" +(a<=b));
	}

}
