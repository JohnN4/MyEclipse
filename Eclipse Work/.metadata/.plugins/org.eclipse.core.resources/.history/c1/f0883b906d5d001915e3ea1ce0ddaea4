/**
 * 
 */
package com.jn.edu;

import java.util.Scanner;

/**
 * @author JohnN
 *
 */
public class Swap {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a = sc.nextInt();
		int b = sc.nextInt();
		System.out.println("a = " + a + ";b = " + b);
		int temp =b;
		b=a;
		a=temp;
		System.out.println("a = "+ a + "; b = "+b);
	}

}
